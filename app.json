{
  "expo": {
    "name": "reactNative",
    "slug": "reactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/helpii_logo.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/helpii_photo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#a24fb0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/helpii_photo.jpeg",
        "backgroundColor": "#a24fb0"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "NÃ£o foi permitido"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": false
          }
        }
      ]
    ]
  }
}
